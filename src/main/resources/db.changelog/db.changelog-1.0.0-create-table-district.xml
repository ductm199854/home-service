<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="create-table-district" author="alpha-home">
        <createTable tableName="APP_DISTRICT">
            <column name="DISTRICT_ID" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="CITY" type="NVARCHAR(70)"/>
            <column name="DISTRICT_NAME" type="NVARCHAR(70)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
